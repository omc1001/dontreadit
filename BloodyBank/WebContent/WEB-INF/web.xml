<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>BloodyBank</display-name>
 <listener>
  <listener-class>mc.BbListener</listener-class>
 </listener>
 <servlet>
  <display-name>BbServlet</display-name>
  <servlet-name>BbServlet</servlet-name>
  <jsp-file>/FormDonator.jsp</jsp-file>
 </servlet>
 <servlet-mapping>
  <servlet-name>BbServlet</servlet-name>
  <url-pattern>/BbServlet</url-pattern>
 </servlet-mapping>
 <welcome-file-list>
  <welcome-file>/index.jsp</welcome-file>
 </welcome-file-list>
</web-app>
