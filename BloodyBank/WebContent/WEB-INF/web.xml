<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>BloodyBank</display-name>
  <listener>
    <listener-class>mc.BbListener</listener-class>
  </listener>
  <servlet>
    <display-name>BbServlet</display-name>
    <servlet-name>BbServlet</servlet-name>
    <jsp-file>/FormDonator.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>BbServlet</servlet-name>
    <url-pattern>/BbServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>mc.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogareServlet</display-name>
    <servlet-name>LogareServlet</servlet-name>
    <servlet-class>mc.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogareServlet</servlet-name>
    <url-pattern>/LogareServlet</url-pattern>
  </servlet-mapping>
</web-app>